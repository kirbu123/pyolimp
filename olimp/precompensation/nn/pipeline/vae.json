{
    "model": {
        "name": "vae"
    },
    "img": {
        "datasets": [
            {
                "name": "SCA2023",
                "subsets": [
                    "Images/Real_images/Faces",
                    "Images/Real_images/Urban"
                ]
            }
        ],
        "transforms": [
            {
                "name": "grayscale",
                "num_output_channels": 1
            },
            {
                "name": "resize",
                "width": 710,
                "height": 444
            },
            {
                "name": "float32"
            },
            {
                "name": "divide",
                "value": 255
            }
        ]
    },
    "distortion": [
        {
            "name": "refraction_datasets",
            "psf": {
                "datasets": [
                    {
                        "name": "SCA2023",
                        "subsets": [
                            "PSFs/Narrow"
                        ]
                    }
                ],
                "transforms": [
                    {
                        "name": "psf_normalize"
                    },
                    {
                        "name": "resize",
                        "width": 710,
                        "height": 444
                    },
                    {
                        "name": "grayscale",
                        "num_output_channels": 1
                    },
                    {
                        "name": "float32"
                    }
                ]
            }
        }
    ],
    "loss_function": {
        "name": "Vae"
    },
    "batch_size": 4,
    "sample_size": 120,
    "optimizer": {
        "name": "Adam",
        "learning_rate": 0.001,
        "eps": 1e-8
    }
}